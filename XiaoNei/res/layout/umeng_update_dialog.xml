<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
    android:layout_height="match_parent" xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:gravity="center"
    android:background="#99232323">
    
<LinearLayout 
    android:layout_width="280dp"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@drawable/shape_card_noshadow" >
    
    <RelativeLayout 
        android:layout_height="wrap_content"
        android:layout_width="match_parent">
        
         <Button
                android:id="@+id/umeng_update_id_close"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="10dp"
				android:focusable="true"
				android:background="@drawable/umeng_update_button_close_bg_selector"/>
	    <TextView 
	        android:layout_height="wrap_content"
	        android:layout_width="match_parent"
	        android:paddingLeft="@dimen/dialog_mytucao_delete_text_padding_left"
	        android:paddingTop="@dimen/dialog_mytucao_delete_text_padding_v"
	        android:paddingBottom="@dimen/dialog_update_introduce_text_padding"
	        android:textSize="@dimen/dialog_mytucao_delete_text_size"
	        android:textColor="@color/text_dialog_mytucao_delete_title"
	        android:text="发现新版本"/>
    
    </RelativeLayout>
    
    <TextView 
        android:id="@+id/umeng_update_content"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:paddingLeft="@dimen/dialog_mytucao_delete_text_padding_left"
        android:paddingTop="@dimen/dialog_update_introduce_text_padding"
        android:paddingBottom="@dimen/dialog_mytucao_delete_text_padding_v"
        android:text="·更新大量功能"
        android:textSize="@dimen/dialog_update_introduce_text_size"
        android:textColor="@color/text_gray_dark"/>
    
    <ImageView 
        android:layout_width="@dimen/dialog_mytucao_delete_line_width"
        android:layout_height="@dimen/dialog_mytucao_delete_line_height"
        android:layout_marginLeft="@dimen/dialog_mytucao_delete_line_padding_h"
        android:layout_marginRight="@dimen/dialog_mytucao_delete_line_padding_h"
        android:background="@color/line"/>
    
    <Button 
        android:id="@+id/umeng_update_id_ok"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/dialog_mytucao_delete_text_padding_left"
        android:paddingTop="@dimen/dialog_mytucao_delete_button_padding_v"
        android:paddingBottom="@dimen/dialog_mytucao_delete_button_padding_v"
        android:gravity="left"
        android:textSize="@dimen/dialog_mytucao_delete_button_text_size"
        android:textColor="@color/text_black_dark"
        android:text="更新"
        android:background="@drawable/selector_cover_transparent"
        />
    
    <RelativeLayout 
        android:layout_height="0dp"
        android:layout_width="0dp">
        <ImageView 
            android:id="@+id/umeng_update_wifi_indicator"
            android:layout_height="0dp"
            android:layout_width="0dp"/>
        <Button 
            android:id="@+id/umeng_update_id_cancel"
            android:layout_height="0dp"
            android:layout_width="0dp"/>
        <Button 
            android:id="@+id/umeng_update_id_ignore"
            android:layout_height="0dp"
            android:layout_width="0dp"/>
        <CheckBox 
            android:id="@+id/umeng_update_id_check"
            android:layout_height="0dp"
            android:layout_width="0dp"
            android:button="@drawable/umeng_update_button_check_selector"/>
        
    </RelativeLayout>
    

</LinearLayout>

</RelativeLayout>
